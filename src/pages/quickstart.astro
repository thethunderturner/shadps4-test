---
import {getEntry, render} from 'astro:content';
import MarkdownLayout from '@/layouts/MarkdownLayout.astro';

const wiki = await getEntry('wiki', 'quickstart');
const {Content, headings} = await render(wiki);
---

<MarkdownLayout frontmatter={wiki.data} headings={headings}>
    <Content />
</MarkdownLayout>
